{
  "name": "async-object",
  "version": "1.0.0",
  "description": "Asynchronous object written in vanilla JS",
  "main": "dist/async-object.js",
  "license": "MIT",
  "files": [
    "*.js",
    "*.md",
    "umd",
    "commonjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/petlack/async-object.git"
  },
  "homepage": "https://github.com/petlack/async-object#readme",
  "author": "Peter Laca <peter@output.sk>",
  "keywords": [
    "javascript",
    "async",
    "utility",
    "es6",
    "universal"
  ],
  "bugs": {
    "url": "https://github.com/petlack/async-object/issues"
  },
  "contributors": [
    {
      "name": "Peter Laca",
      "url": "https://github.com/petlack"
    }
  ],
  "scripts": {
    "build": "babel-node ./tools/scripts/build.js",
    "check": "lint-staged",
    "clean": "rimraf ./commonjs && rimraf ./umd && rimraf ./coverage && rimraf ./flow-coverage && rimraf ./umd",
    "lint": "eslint src",
    "precommit": "yarn run check",
    "prepublish": "yarn run build",
    "prepush": "yarn run test",
    "prettify": "node_modules/.bin/prettier-eslint --write",
    "prettify:all": "find . -type f -name \"*.js\" -not -path \"./node_modules/*\" -not -path \"./commonjs/*\" -not -path \"./umd/*\" | xargs yarn run prettify --",
    "reinstall": "rm -rf node_modules && rm yarn.lock && yarn install",
    "test": "jest",
    "test:coverage": "yarn run test -- --coverage",
    "test:coverage:deploy": "yarn run test:coverage && codecov"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(commonjs|coverage|flow-typed|node_modules|tools|umd)/"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {
    "app-root-dir": "1.0.2",
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.3.2",
    "babel-preset-latest": "6.24.0",
    "babel-preset-stage-3": "6.22.0",
    "babel-register": "6.24.0",
    "chai": "^3.5.0",
    "codecov": "2.1.0",
    "cross-env": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "gzip-size": "3.0.0",
    "in-publish": "2.0.0",
    "jest": "19.0.2",
    "lint-staged": "^3.4.0",
    "prettier": "^0.22.0",
    "prettier-eslint": "4.4.0",
    "prettier-eslint-cli": "3.2.0",
    "pretty-bytes": "4.0.2",
    "ramda": "0.23.0",
    "readline-sync": "1.4.7",
    "rimraf": "2.6.1",
    "sinon": "2.1.0",
    "webpack": "2.3.3",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.18.0"
  }
}
